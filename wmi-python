import wmi
 
c = wmi.WMI()   
my_system = c.Win32_ComputerSystem()[0]
two_system = c.Win32_Bios()[0]
os_system = c.Win32_OperatingSystem()[0]
 
print(f"SerialNumber: {two_system.SerialNumber}")
print(f"HostName: {my_system.Name}")
print(f"Manufacturer: {my_system.Manufacturer}")
print(f"Model: {my_system. Model}")
print(f"NumberOfProcessors: {my_system.NumberOfProcessors}")
print(f"SystemType: {my_system.SystemType}")
print(f"Windows: {os_system.BuildType}")
print(f"Windows: {os_system.InstallDate}")
print(f"Windows: {os_system.Version}")
print(f"xxx > {os_system.Name}")
